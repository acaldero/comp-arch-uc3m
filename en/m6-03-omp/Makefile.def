EN_M6_03_OMP_BASE=m6-03-omp
EN_M6_03_OMP_DIR=en/$(EN_M6_03_OMP_BASE)
INT_M6_03_OMP_DIR=int/$(EN_M6_03_OMP_BASE)
EN_M6_03_OMP_PARTS=\
  $(EN_M6_03_OMP_DIR)/*.tex \
  $(INT_M6_03_OMP_DIR)/examples/firstprivate/*.cpp \
  $(INT_M6_03_OMP_DIR)/examples/lastprivate/*.cpp \

EN_M6_03_OMP_LECT_PDF=en-$(EN_M6_03_OMP_BASE)-lect.pdf
EN_M6_03_OMP_HOUT_PDF=en-$(EN_M6_03_OMP_BASE)-hout.pdf

FINAL_EN_M6_03_OMP_LECT_PDF=$(FINAL_DIR)/$(EN_M6_03_OMP_LECT_PDF)
FINAL_EN_M6_03_OMP_HOUT_PDF=$(FINAL_DIR)/$(EN_M6_03_OMP_HOUT_PDF)

ALL_SLIDES_EN_FINAL_PDF += \
$(FINAL_DIR)/$(EN_M6_03_OMP_LECT_PDF) \
$(FINAL_DIR)/$(EN_M6_03_OMP_HOUT_PDF) \
